.loginSpan{
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  background-color: #f1f1f5;
  color: black;
  font-size: 12px;
  padding: 16px 30px;
  border: none;
  cursor: pointer;
  border-radius: 5px;
  text-align: center;
  margin-top: 3.5em;
  width: 9em;
}

.loginSpan:hover{
	background-color: black;
  	color: white;
}
.modal{
	position: fixed;
	 z-index: 10001;
}
.regMember{
	font-size: 2em;
	text-align: center;
	margin-left: 9.4em;
}
.regMember-memInfo, .regMember-logInfo {
	font-size: 1.5em;
	text-decoration: underline;
}
.logInfo {
	font-size: 1.2em;
	margin-top: 0.5em;
}
.btn {
	font-size: 0.8em;
	height: 4em;
}
.tabInput {
	width: 16em;	
}
.genderRadio {
	font-size: 1.5em;
	margin-top: 0.5em;
}
.modal-lg {
	width: 650px;
}
.text-end {
	text-align: right;
}
.loginBtnDiv {
	text-align: center;	
}
.findSpan{
	 cursor: pointer;
}
